{
  "name": "custom-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "all",
        "path": "./global.json"
      },
      {
        "language": [
          "typescript",
          "javascript"
        ],
        "path": "./jsdoc.json"
      },
      {
        "language": [
          "go"
        ],
        "path": "./go.json"
      },
      {
        "language": [
          "gleam"
        ],
        "path": "./gleam.json"
      },
      {
        "language": [
          "rust"
        ],
        "path": "./rust.json"
      },
      {
        "language": [
          "svelte",
          "typescript",
          "javascript"
        ],
        "path": "./javascript.json"
      },
      {
        "language": [
          "svelte",
          "html"
        ],
        "path": "./html.json"
      },
      {
        "language": [
          "svelte"
        ],
        "path": "./svelte.json"
      }
    ]
  }
}
